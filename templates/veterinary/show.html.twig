{% block body_id %}fiche
{% endblock %}
{% block body %}
	<h1>Fiche détail vétérinaire n°{{ veto.id }}</h1>
	<div class=" my-3">
		<div class="card">
			<img src="{{ asset('images/vetos/'~ veto.fichierImage ) }}" class="card-img-top" alt="{{ veto.nom }}">
			<div class="card-body">
				<h5 class="card-title">{{ veto.nom }}</h5>
				<p class="card-text">{{ veto.adresse }}
					<br>
					{{ veto.codePostal }}
					{{ veto.ville }}
				</p>
				<p class="card-text">Téléphone :
					{{ veto.telephone }}</p>
			</div>
		</div>
		<ul class="liens list-unstyled mt-3">
			<li>
				<a href="{{ path('app_veterinary') }}">
					<i class="bi bi-chevron-left"></i>
					Retour à la liste
				</a>
			</li>
		</ul>
	</div>
{% endblock %}
