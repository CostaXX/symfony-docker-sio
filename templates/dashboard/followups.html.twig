{% extends 'base.html.twig' %}
{% block title %}Suivis par vétérinaire
{% endblock %}
{% block body %}
	<h1>Nombre de suivis par vétérinaire</h1>
	<table class="table">
		<thead>
			<tr>
				<th>Id</th>
				<th>Vétérinaire</th>
				<th>Adresse</th>
				<th>Nombre de suivis</th>
			</tr>
            {% for followup in followups %}
            <tr>
                <td>{{ followup.id }}</td>
                <td>{{ followup.name }}</td>
                <td>{{ followup.address }}</td>
                <td>{{ followup.number }}</td>
                
            </tr>
        {% else %}
            <tr>
                <td colspan="9">no records found</td>
            </tr>
        {% endfor %}
		</thead>
	</table>
{% endblock %}
